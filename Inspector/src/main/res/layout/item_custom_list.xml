<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:descendantFocusability="blocksDescendants"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/dimen_size_15"
        android:layout_marginTop="@dimen/dimen_size_15"
        android:layout_marginEnd="@dimen/dimen_size_15"
        android:background="@drawable/rec_bg_white_r5"
        android:gravity="center_vertical"
        android:padding="@dimen/dimen_size_15">

        <TextView
            android:id="@+id/item_custom_title"
            style="@style/Font15_black"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="标题" />


        <ImageView
            android:id="@+id/item_custom_edit"
            android:layout_width="@dimen/dimen_size_30"
            android:layout_height="@dimen/dimen_size_30"
            android:padding="@dimen/dimen_size_5"
            android:src="@mipmap/index_icon11"
            tools:ignore="ContentDescription" />

        <ImageView
            android:id="@+id/item_custom_del"
            android:layout_width="@dimen/dimen_size_30"
            android:layout_height="@dimen/dimen_size_30"
            android:layout_marginStart="@dimen/dimen_size_5"
            android:padding="@dimen/dimen_size_5"
            android:src="@mipmap/index_icon12"
            tools:ignore="ContentDescription" />

    </LinearLayout>

</LinearLayout>